{
  "name": "levo-sso",
  "version": "1.0.0",
  "private": true,
  "description": "LEVO Single Sign-On Service",
  "scripts": {    
    "test": "npm run build && npm run test:all",
    "test:all": "mocha test/*.mocha.js --recursive src/**/*.spec.js --timeout 5000",
    "test:spec": "mocha --recursive src/**/*.spec.js --timeout 5000",
    "test:react": "mocha -r test/helpers/browser.js --recursive client/**/*.spec.js",
    "cover": "nyc --reporter=html --reporter=text npm run test:all",
    "bulid:clean": "rm -rf server",
    "build": "npm run bulid:clean && NODE_ENV=development babel src -d server --copy-files && del server/**/__test__",
    "build:prod": "npm run bulid:clean && NODE_ENV=production babel src -d server --copy-files && del server/**/__test__",
    "build:watch": "NODE_ENV=development babel src -w -d server --copy-files",
    "build:client": "echo skipped",
    "build:client:prod": "webpack --config conf/webpack.product.js",
    "build:client:watch": "echo skipped",
    "oolong:build": "oolong build -c conf/app.default.json",
    "oolong:migrate": "npm run build && oolong migrate -c conf/app.default.json -r"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-plugin-contract": "^1.3.2",
    "babel-plugin-source-map-support": "^2.0.1",
    "del-cli": "^1.1.0",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "should": "^13.2.3",
    "source-map-support": "^0.5.9",
    "supertest": "^3.3.0",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
  }
}
